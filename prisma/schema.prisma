// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FileNode {
  id        String       @id @default(uuid())
  name      String
  type      FileNodeType
  createdAt DateTime     @default(now())
}

enum FileNodeType {
  DIRECTORY
  FILE
  FILE_VERSION
}
